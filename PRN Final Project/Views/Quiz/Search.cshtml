    <header>
        <div class="header-area">
            <div class="main-header header-sticky">
                <div class="container-fluid">
                    <div class="menu-wrapper d-flex align-items-center justify-content-between">
                        <div class="left-content d-flex align-items-center">
                            <div class="logo">
                                <a href="/"><img src="/Assets/img/logo/logo.png" alt=""></a>
                            </div>

                            <form action="search.html" class="form-box">
                                <input type="text" name="Search" placeholder="Search courses..">
                                <div class="search-icon">
                                    <i class="ti-search"></i>
                                </div>
                            </form>
                        </div>

                        <div class="main-menu d-none d-lg-block">
                            <nav>
                                <ul id="navigation">
                                    <li><a href="index-2.html">Trang chủ</a></li>
                                    <li><a href="courses.html">Thư viện của bạn</a></li>

                                    <li>
                                        <a href="add.html" class="quiz-add"> Học phần mới </a>
                                    </li>
                                    <li>
                                        <a href="login.html" class="btn header-btn2">Sign In</a>
                                    </li>
                                    <li>
                                        <a href="register.html" class="btn header-btn2">Sign Up</a>
                                    </li>
                                </ul>
                            </nav>
                        </div>
                    </div>

                    <div class="col-12">
                        <div class="mobile_menu d-block d-lg-none"></div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <main>

        <div class="hero-area slider-area">
            <div class="hero-height2 hero-bg2 d-flex align-items-center">
                <div class="container">
                    <div class="row">
                        <div class="col-xl-12">
                            <div class="hero-cap">
                                <h2>Học phần của bạn</h2>
                                <nav aria-label="breadcrumb">
                                    <ol class="breadcrumb">
                                        <li class="breadcrumb-item"><a href="/">Trang chủ</a></li>
                                        <li class="breadcrumb-item"><a href="#">Search</a></li>
                                        <li class="breadcrumb-item"><a href="#">@Request["keyword"]</a></li>
                                    </ol>
                                </nav>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <section class="popular-directorya-area section-padding fix">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">

                        <div class="section-tittle text-center mb-40">
                            <h2>Kết quả cho "@Request["keyword"]"</h2>
                            <p>Đã tìm thấy @ViewBag.Result.Count kết quả</p>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-12 col-md-2">

                        @*<form action="search.html" class="quiz-search-form">
                                <input type="text" name="Search" placeholder="Search courses.." class="quiz-search-field">
                                <div class="search-icon quiz-search-button">
                                    <button type="submit"><i class="ti-search"></i></button>

                                </div>
                            </form>
                            <div class="row quiz-sort-Title">
                                <h4><i class="fas fa-list"></i> List By</h4>
                            </div>
                            <div class="row">
                                <table class="quiz-table-sort-by">

                                    <tr>
                                        <td><input type="checkbox" name="sortByDay" id="" value="nearest"></td>
                                        <td><label for="">Gần đây nhất</label></td>
                                    </tr>
                                    <tr>
                                        <td><input type="checkbox" name="sortByRate" id="" value="rate"></td>
                                        <td><label for="">Đánh Giá</label></td>
                                    </tr>
                                    <tr>
                                        <td><input type="checkbox" name="sortByRate" id="" value="view"></td>
                                        <td><label for="">Lượt truy cập</label></td>
                                    </tr>

                                </table>
                            </div>*@

                    </div>
                    <div class="col-sm-12 col-md-8">
                        @if (ViewBag.Result.Count > 0)
                        {
                            foreach (Quiz quiz in ViewBag.Result)
                            {
                                <div class="row quiz-search-item">
                                    <div class="row quiz-item-overview">
                                        <div class="col-sm-3 quiz-search-amount">
                                            @quiz.TermAmount Thuật ngữ
                                        </div>
                                        <div class="col-sm-3 quiz-search-author">
                                            by <a href="courses.html">@quiz.Creator</a>
                                        </div>
                                        <div class="col-sm-3 quiz-rating">

                                        </div>
                                        <div class="col-sm-3 quiz-view">

                                        </div>
                                    </div>
                                    <div class="row quiz-item-info">
                                        <div class="col-sm-2">
                                            <h3><a href="quiz.html">@quiz.QuizName</a></h3>
                                        </div>
                                        <div class="col-sm-8">
                                            <p>@quiz.QuizDescription</p>
                                        </div>

                                    </div>
                                </div>
                            }
                        }
                        else
                        {
                            <br />
                            <h3 style="text-align: center">Không có kết quả nào được tìm thấy</h3>
                        }



                    </div>
                    <div class="col-sm-12 col-md-2">
                    </div>



                </div>

        </section>

    </main>

    @Html.Partial("~/Views/Shared/_Footer.cshtml")

    <div id="back-top">
        <a class="wrapper" title="Go to Top" href="#">
            <div class="arrows-container">
                <div class="arrow arrow-one">
                </div>
                <div class="arrow arrow-two">
                </div>
            </div>
        </a>
    </div>


